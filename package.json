{
  "name": "react-storybook-library",
  "version": "1.0.0",
  "description": "Template for a React TypeScript npm package",
  "author": "Mykhailo Hudzevatyi @michaelgudzevskyi",
  "private": false,
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ], 
  "scripts": {
    "start": "webpack-dev-server --open  --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.prod.js",
    "watch": "tsc -w",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o demo"
  },
  "dependencies": {
    "react": "^17.0.1"
  },
  "devDependencies": {
    "react-dom": "^17.0.1",
    "@storybook/addon-actions": "^6.2.5",
    "@storybook/addon-links": "^6.2.5",
    "@storybook/addons": "^6.2.5",
    "@storybook/react": "^6.2.5",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-prettier-react": "0.0.24",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.4.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "homepage": "https://github.com/michaelgudzevskyi/react-storybook-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelgudzevskyi/react-storybook-library.git"
  },
  "bugs": {
    "url": "https://github.com/michaelgudzevskyi/react-storybook-library/issues",
    "email": "hello@michaelgudzevskyi.com"
  },
  "keywords": [
    "storybook",
    "react",
    "reactjs",
    "typescript",
    "ui-components",
    "library",
    "npm",
    "npm-package"
  ]
}
